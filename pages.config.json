{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "buildConfig": {
    "nodeVersion": "18.17.1"
  },
  "build": {
    "command": "npm run build",
    "environment": {
      "NODE_VERSION": "18.17.1"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_KEY": "",
    "NEXT_PUBLIC_BASE_URL": "",
    "NEXT_PUBLIC_MODEL": ""
  }
}
